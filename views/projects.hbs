<main role="main">

  <div>
    <div class="album py-5 bg-light">
      <div class="container">

        <div class="row">
          {{#each projects}}
          <div class="col-md-4">
            <div class="card mb-4 shadow-sm">
              <img class="card-img-top" src="{{this.imageUrl}}" alt="Card image cap">
              <div class="card-body">
                <h3 class="card-head">{{this.name}}</h3>
                <p class="card-text">{{this.description}}</p>
                <div class="d-flex justify-content-between align-items-center">
                  <div class="btn-group">
                    <a href="/projects/{{this.alias}}" class="btn btn-sm btn-outline-secondary">Detail</a>
                    <a href="{{this.githubUrl}}" target="_blank" class="btn btn-sm btn-outline-secondary">Github</a>
                  </div>
                  <small class="text-muted">9 mins</small>
                </div>
              </div>
            </div>
          </div>
          {{/each}}

        </div>
      </div>
    </div>
  </div>
</main>



